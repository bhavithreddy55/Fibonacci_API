# .env file (generic template)
HOST=<host>           # Replace <host> with the actual host (e.g., 0.0.0.0)
PORT=<port>           # Replace <port> with the actual port (e.g., 8000)
HOST_PORT=<host_port> # Replace <host_port> with the host machine port (e.g., 5000)
CONTAINER_PORT=<container_port> # Replace <container_port> with the container port (e.g., 8000)